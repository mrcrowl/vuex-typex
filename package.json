{
  "name": "vuex-typex",
  "version": "3.1.6",
  "description": "A TypeScript pattern for strongly-typed access to Vuex Store modules",
  "files": [
    "dist/index.js",
    "dist/index.d.ts",
    "dist/index.js.map"
  ],
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "peerDependencies": {
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@types/chai": "^3.4.35",
    "@types/chai-http": "^0.0.30",
    "@types/jest": "^23.3.13",
    "@types/mocha": "^2.2.40",
    "axios": "^0.18.1",
    "chai": "^3.5.0",
    "growl": "^1.10.5",
    "jest": "^23.0.0",
    "jest-config": "^24.0.0",
    "mocha": "^3.2.0",
    "nyc": "^10.1.2",
    "rimraf": "^2.6.3",
    "ts-jest": "^23.10.5",
    "ts-loader": "^5.3.3",
    "typescript": "^2.5.0",
    "vue": "^2.5.22",
    "vuex": "^3.0.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mrcrowl/vuex-typex.git"
  },
  "keywords": [
    "vuex",
    "typescript",
    "builder"
  ],
  "author": "mrcrowl",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mrcrowl/vuex-typex/issues"
  },
  "homepage": "https://github.com/mrcrowl/vuex-typex#readme",
  "scripts": {
    "clean": "rimraf dist",
    "compile": "tsc --pretty",
    "build": "npm run clean && npm run compile && rimraf dist/examples && rimraf dist/tests",
    "COMMENTS": "\"test-only\": \"mocha --recursive dist/**/*.spec.js\",",
    "test:unit": "jest",
    "test": "npm run test:unit"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "transform": {
      "^.+\\.tsx?$": "test-jest"
    },
    "testURL": "http://localhost/",
    "testRegex": "src/tests/.*\\.spec.ts$"
  }
}
